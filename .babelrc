{
    "env": {
        "development": {
            "presets": [
                "next/babel"
            ]
        },
        "production": {
            "presets": [
                "next/babel"
            ]
        },
        "test": {
            "presets": [
                "@babel/env",
                "@babel/react",
                "@babel/preset-typescript"
            ],
            "plugins": [
                [
                    "@babel/plugin-transform-modules-commonjs",
                    {
                        "allowTopLevelThis": true
                    }
                ]
                ]
        }
    }
}